<tr>
  <th><%= kitten.name %></th>
  <td><%= kitten.age %></td>
  <td><%= kitten.cuteness %></td>
  <td><%= kitten.softness %></td>
  <td>
      <div>
          <%= link_to '<i class="fa fa-search mr-2"></i>'.html_safe, kitten %>
          <%= link_to '<i class="fa fa-edit mr-2"></i>'.html_safe, edit_kitten_path(kitten) %>
          <%= link_to '<i class="fa fa-trash"></i>'.html_safe, kitten, method: :delete, data: { confirm: 'Are you sure?' } %>
      </div>
  </td>
</tr>
